{
  "expo": {
    "name": "SonaClientFrontend",
    "slug": "SonaClientFrontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "sonaclientfrontend",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "jsEngine": "hermes",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.SonaClientFrontend",
      "infoPlist": {
        "NSCameraUsageDescription": "Camera is required for video calls.",
        "NSMicrophoneUsageDescription": "Microphone is required for audio in video calls.",
        "NSPhotoLibraryUsageDescription": "Photo library access is required to choose profile images (optional).",
        "LSApplicationQueriesSchemes": [
          "jitsi-meet",
          "intent"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "CAMERA",
        "RECORD_AUDIO",
        "MODIFY_AUDIO_SETTINGS",
        "INTERNET",
        "CAMERA",
        "RECORD_AUDIO",
        "MODIFY_AUDIO_SETTINGS",
        "INTERNET"
      ],
      "package": "com.anonymous.SonaClientFrontend"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "002c72ed-cacf-41c5-9401-248921c859cd"
      }
    }
  }
}
